{
  "name": "wing-vue",
  "version": "0.0.1",
  "description": "Wing，一个轻量级的vue组件库",
  "main": "./lib/wing.umd.js",
  "module": "./lib/wing.esm.js",
  "unpkg": "./lib/wing.min.js",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "/src",
    "/lib"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "dist": "npm run clean",
    "build": "npm run clean && rollup -c ./build/rollup.config.js",
    "build:es": "npm run clean && rollup -c ./build/rollup.es.js",
    "build:umd": "npm run clean && rollup -c ./build/rollup.umd.js",
    "build:iife": "npm run clean && rollup -c ./build/rollup.iife.js",
    "build:packages": "npm run clean && rollup -c ./build/rollup.packages.js",
    "create": "node ./build/createComponent.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test": "jest"
  },
  "keywords": [
    "wing",
    "vue",
    "ui"
  ],
  "author": "rust404",
  "license": "ISC",
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "@wing-ui/icons-vue": "^0.0.2",
    "vue": "^2.6.12",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/generator": "^7.11.4",
    "@babel/parser": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/traverse": "^7.11.0",
    "@babel/types": "^7.11.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.0-rc.9",
    "@vue/test-utils": "^1.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "css-loader": "^4.2.2",
    "expect": "^26.4.2",
    "html-webpack-plugin": "^4.3.0",
    "inquirer": "^7.3.3",
