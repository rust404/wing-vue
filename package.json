    "progress-bar-webpack-plugin": "^2.1.0",
    "puppeteer": "^5.2.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-vue": "5.1.9",
    "sass": "^1.26.10",
