{
  "name": "wing-vue",
  "version": "0.0.1",
  "description": "Wing，一个轻量级的vue组件库",
  "main": "lib/wing.js",
  "scripts": {
    "clean": "rimraf lib",
    "dist": "npm run clean",
    "build:packages": "npm run clean && webpack --config build/webpack.component.js",
    "build:all": "npm run clean && webpack --config build/webpack.build.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "dev": "webpack-dev-server --inline --config build/webpack.dev.js",
    "test": "mochapack --webpack-config ./build/webpack.test.js --require test/setup.js src/packages/**/__test__/**.spec.js"
  },
  "keywords": [
    "wing",
    "vue",
    "ui"
  ],
  "author": "rust404",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.6.12",
    "webpack": "^4.44.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/test-utils": "^1.0.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "css-loader": "^4.2.2",
    "expect": "^26.4.2",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^0.10.0",
    "mocha": "^8.1.2",
    "mochapack": "^2.0.3",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "puppeteer": "^5.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.5.3",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.2",
    "webpack-node-externals": "^2.5.2"
  }
}
